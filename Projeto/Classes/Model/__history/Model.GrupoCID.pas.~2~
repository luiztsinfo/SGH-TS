unit Model.GrupoCID;

interface

uses
  Lca.Orm.Base, Lca.Orm.Atributos;

type
  [attTabela('GRUPOS_CID')]
  TGrupos_cid = class(TTabela)
  private
    FId: Integer;
    FDescricao: string;
  public
    [attPK]
    property Id: Integer read FId write FId;
    property Descricao: string read FDescricao write FDescricao;
  end;

implementation

end.

