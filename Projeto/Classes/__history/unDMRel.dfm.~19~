      'FROM he '
      'LEFT JOIN pessoas pac'
      'ON pac.id = he.id_paciente'
      'LEFT JOIN pessoas enf'
      'ON enf.id = he.id_enfermeiro'
      'LEFT JOIN religioes rel'
      'ON rel.id = pac.id_religiao'
      'LEFT JOIN antecedentes_paciente ap'
